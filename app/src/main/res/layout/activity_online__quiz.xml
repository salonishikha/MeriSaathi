<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_online__quiz"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.example.acer.merisaathi.Online_Quiz">
    <RadioGroup
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/security_service_form_rg1"
        android:checkedButton="@+id/security_service_form_rb2"
        android:orientation="horizontal">
        <RadioButton
            android:id="@+id/security_service_form_rb1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="male"/>
        <RadioButton
            android:id="@+id/security_service_form_rb2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="female"/>
        <RadioButton
            android:id="@+id/security_service_form_rb3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="others"/>

    </RadioGroup>

</LinearLayout>
